<Window
    x:Class="Foundation.Views.SearchEntityForm"
    Name="ThisWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"

    xmlns:local="clr-namespace:Foundation.Views"
    xmlns:controls="clr-namespace:Foundation.Views"
    
    mc:Ignorable="d"
    SizeToContent="WidthAndHeight"
    Title="Entity Search Form"
    MinHeight="235"
    MinWidth="272"
    >
        
    <StackPanel>
        <controls:AvailableSelectedControl AvailableListItems="{Binding AvailableListItems}" SelectedListItems="{Binding SelectedListItems}" />
    </StackPanel>
</Window>
